<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="cn.husestudy.sendmsg.vo.SendMsg" table="sendmsg">
		<id name="sendmsg_id">
			<generator class="native"/>
		</id>
		<property name="sendmsg_name"/>
		<property name="sendmsg_email"/>
		<property name="sendmsg_message"/>
		<property name="sendmsg_sdate"/>
		
		
	</class>
</hibernate-mapping>